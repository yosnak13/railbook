<table>
  <tr>
    <th>ISBNコード</th><tr>書名</tr><tr>価格</tr>
    <th>出版社</th><tr>刊行日</tr><tr>ダウンロード</tr>
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.isbn %></td>
      <td><%= book.title %></td>
      <td><%= book.price %></td>
      <td><%= book.publish %></td>
      <td><%= book.published %></td>
      <td><%= book.dl %></td>
    </tr>
  <% end %>
</table>
